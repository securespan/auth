<script>
var platform = window.navigator.platform;
var appleStoreElements = document.querySelectorAll('[data-select="istore"]');
var playStoreElements = document.querySelectorAll('[data-select="playstore"]');
var downloadElements = document.querySelectorAll('[data-select="download"]');

// Function to check if the device is a tablet or mobile
function isTabletOrMobile() {
    return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
}

// Function to hide download elements
function hideDownloadElements() {
    downloadElements.forEach(function(element) {
        element.classList.add('hide');
    });
}

if (isTabletOrMobile()) {
    // Hide download elements on tablet and mobile
    hideDownloadElements();

    // Check if the device is an Apple device
    if (platform.indexOf('Mac') !== -1 || platform.indexOf('iPhone') !== -1 || platform.indexOf('iPad') !== -1 || platform.indexOf('iPod') !== -1) {
        // Apple device detected, show Apple Store elements
        playStoreElements.forEach(function(element) {
            element.classList.add('hide'); // Hide the Play Store elements
        });
        appleStoreElements.forEach(function(element) {
            element.classList.remove('hide'); // Show the Apple Store elements
        });
        console.log("The user is running on an Apple device");
    } else {
        // Non-Apple device detected, show Play Store elements
        playStoreElements.forEach(function(element) {
            element.classList.remove('hide'); // Show the Play Store elements
        });
        appleStoreElements.forEach(function(element) {
            element.classList.add('hide'); // Hide the Apple Store elements
        });
        console.log("The user is not running on an Apple device");
    }
} else {
    // Desktop device detected, show all elements
    downloadElements.forEach(function(element) {
        element.classList.remove('hide');
    });
    playStoreElements.forEach(function(element) {
        element.classList.remove('hide');
    });
    appleStoreElements.forEach(function(element) {
        element.classList.remove('hide');
    });
}
</script></body></html>