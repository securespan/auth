  function updateNavDropdown(dataHoverValue) {
    const navDropdown = document.querySelectorAll('.nav_dropdown');
    navDropdown.forEach((element) => {
      element.setAttribute('data-hover', dataHoverValue);
    });
    Webflow.ready();
