<script defer>
// Cache dropdown elements to reduce jQuery lookups
$('[close_popup]').on('click', function() {
  $('[popup]').css("display", "none");
});

const dropdownLink = $("[dropdown='link']");
const dropdownContainer = $("[dropdown='container']");
const browserButton = $("[browserButton]");
const mobileOsButton = $("[mobileOsButton]");
const buttonIcons = $("[buttonIcons]");
const buttonText = $("[buttonText]");

// Function to handle the dropdown functionality for desktop only
function initializeDropdownHover() {
  if (window.innerWidth > 768) {
    dropdownLink.on("mouseenter", () => {
      dropdownContainer.css("visibility", "visible").addClass("is-dd-active");
    });

    dropdownLink.on("mouseleave", () => {
      dropdownContainer.removeClass("is-dd-active");
      setTimeout(() => dropdownContainer.css("visibility", "hidden"), 100);
    });
  }
}

// Detect user's browser and set appropriate button link
function setBrowserButton() {
  const userAgent = navigator.userAgent || navigator.vendor || window.opera;
  const isMobile = /iPhone|iPad|iPod|Android/i.test(userAgent);

  if (isMobile) {
    browserButton.find(buttonText).text("Get Extension");
    browserButton.attr("href", "/get");
  } else {
    if (userAgent.match(/edg/i)) {
      browserButton.find(buttonIcons).css("display", "flex");
      browserButton.find("[browserIcon='Edge']").show();
      browserButton.find(buttonText).text("Get for Edge");
      browserButton.attr("href", "https://microsoftedge.microsoft.com/addons/detail/keplr/ocodgmmffbkkeecmadcijjhkmeohinei");
    } else if (userAgent.match(/firefox|fxios/i)) {
      browserButton.find(buttonIcons).css("display", "flex");
      browserButton.find("[browserIcon='Firefox']").show();
      browserButton.find(buttonText).text("Get for Firefox");
      browserButton.attr("href", "https://addons.mozilla.org/en-US/firefox/addon/keplr/");
    } else if (userAgent.match(/chrome|chromium|crios/i)) {
      browserButton.find(buttonIcons).css("display", "flex");
      browserButton.find("[browserIcon='Chrome']").show();
      browserButton.find(buttonText).text("Get for Chrome");
      browserButton.attr("href", "https://chromewebstore.google.com/detail/keplr/dmkamcknogkgcdfhhbddcghachkejeap?hl=en");
    } else {
      browserButton.find(buttonText).text("Get Extension");
      browserButton.attr("href", "/get");
    }
  }
}

// Detect user's mobile OS and set appropriate button link
function setMobileOsButton() {
  const userAgent = navigator.userAgent || navigator.vendor || window.opera;

  if (userAgent.match(/iPad|iPhone|iPod/i)) {
    mobileOsButton.find(buttonIcons).css("display", "flex");
    mobileOsButton.find("[mobileIcon='ios']").show();
    mobileOsButton.find(buttonText).text("Get for iOS");
    mobileOsButton.attr({
      href: "https://apps.apple.com/us/app/keplr-wallet/id1567851089",
      title: "Get Keplr Wallet for iOS",
    });
  } else if (userAgent.match(/Android/i)) {
    mobileOsButton.find(buttonIcons).css("display", "flex");
    mobileOsButton.find("[mobileIcon='android']").show();
    mobileOsButton.find(buttonText).text("Get for Android");
    mobileOsButton.attr({
      href: "https://play.google.com/store/apps/details?id=com.chainapsis.keplr&hl=en",
      title: "Get Keplr Wallet for Android",
    });
  } else {
    mobileOsButton.find(buttonText).text("Get App");
   	$("[mobileDesktopIcons]").css("display", "flex");
    mobileOsButton.on("click", () => {
      $("[popup]").css("display", "flex");
    });
  }
}

$(document).ready(() => {
  setBrowserButton();
  setMobileOsButton();
  initializeDropdownHover();
});
</script>

<script defer>
document.addEventListener("DOMContentLoaded", () => {
  const canvases = document.querySelectorAll("canvas[dots-pattern]");

  if (canvases.length === 0) {
    console.error("No canvases found with the 'dots-pattern' attribute.");
    return;
  }

  const randomSeed = [];
  const timeOffsets = [];
  const maxSeeds = 10000;

  for (let i = 0; i < maxSeeds; i++) {
    randomSeed.push(Math.random());
    timeOffsets.push(Math.random() * Math.PI * 2);
  }

  const DOT_ANIMATION_SPEED = 0.025;
  const dotSize = 4;
  const dotSpacing = 12;
  const color = "#F5F5F5";
  let time = 0;

  function resizeCanvas(canvas) {
    const ctx = canvas.getContext("2d");
    const container = canvas.parentElement;
    const scale = window.devicePixelRatio || 1;

    canvas.width = container.clientWidth * scale;
    canvas.height = container.clientHeight * scale;
    ctx.scale(scale, scale);

    canvas.style.width = `${container.clientWidth}px`;
    canvas.style.height = `${container.clientHeight}px`;
  }

  canvases.forEach(resizeCanvas);
  window.addEventListener("resize", () => canvases.forEach(resizeCanvas));

  function drawAllCanvases() {
    const scale = window.devicePixelRatio || 1;

    canvases.forEach((canvas) => {
      const ctx = canvas.getContext("2d");
      ctx.clearRect(0, 0, canvas.width / scale, canvas.height / scale);

      const rows = Math.ceil(canvas.height / scale / dotSpacing);
      const cols = Math.ceil(canvas.width / scale / dotSpacing);

      for (let y = 0; y < rows; y++) {
        for (let x = 0; x < cols; x++) {
          const posX = x * dotSpacing;
          const posY = y * dotSpacing;
          const index = (x + y * cols) % maxSeeds;

          const seed = randomSeed[index];
          const timeOffset = timeOffsets[index];

          const opacity = (Math.sin(time * DOT_ANIMATION_SPEED + seed * Math.PI + timeOffset) + 1) / 2;
          ctx.fillStyle = color;
          ctx.globalAlpha = opacity;
          ctx.beginPath();
          ctx.arc(posX, posY, dotSize / 2, 0, Math.PI * 2);
          ctx.fill();
          ctx.globalAlpha = 1;
        }
      }
    });

    time += 1;
    requestAnimationFrame(drawAllCanvases);
  }

  drawAllCanvases();
});
</script>

